/**
 *  Exclusive-or gate: out = !(a == b).
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    // Put your code here:
    Nand (a=a, b=a, out=nota);
    Nand (a=b, b=b, out=notb);
    Nand (a=a, b=notb, out=c);
    Nand (a=nota, b=b, out=d);
    Nand (a=c, b=d, out=out);
}